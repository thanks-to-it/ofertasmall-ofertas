{
  "name": "thanks-to-it/ofertasmall-ofertas",
  "keywords": [
    "wordpress",
    "plugin",
    "ofertasmall",
    "lojas"
  ],
  "require": {
    "php": ">=5.3.2",
    "composer/installers": ">=v1.2.0",
    "tareq1988/wordpress-settings-api-class": "dev-master",
    "a5hleyrich/wp-background-processing" : "dev-master"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}": [
        "type:wordpress-plugin"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "classes/"
    ]
  }
}